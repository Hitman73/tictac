@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div> 
    <table>
        <tr class="header">
            <td><p>Победитель</p></td>
            <td><p>Кол-во ходов</p></td>
            <td><p>Дата</p></td>
        </tr>
        @if (ViewBag.List != null)
        {
            foreach (var b in ViewBag.List)
                {
                <tr>
                    @switch ((int)@b.winer)
                    {
                        case 1: <td><p>Ничья</p></td>break;
                        case 2: <td><p>Крестики</p></td>break;
                        case 3: <td><p>Нолики</p></td>break;
                    }
                    <td><p>@b.countStep</p></td>
                    <td><p>@b.date</p></td>
                </tr>
            }
                    }
    </table>

    <p> % побед пользователя (Х) - <b>@ViewBag.Proc</b></p>
</div>